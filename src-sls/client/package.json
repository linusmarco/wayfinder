{
    "name": "wayfinder-client",
    "version": "0.1.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/linusmarco/wayfinder.git"
    },
    "author": "Linus Marco",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/linusmarco/wayfinder/issues"
    },
    "homepage": "https://github.com/linusmarco/wayfinder#readme",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "vue": "^2.5.16",
        "vuex": "^3.0.1",
        "d3": "^5.5.0",
        "d3-delaunay": "^3.1.7",
        "d3-tile": "^0.0.4"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.0.0-beta.15",
        "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
        "@vue/cli-service": "^3.0.0-beta.15",
        "@vue/eslint-config-prettier": "^3.0.0-rc.3",
        "vue-template-compiler": "^2.5.16"
    },
    "prettier": {
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "useTabs": false
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/prettier"
        ],
        "rules": {
            "no-console": 0
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
